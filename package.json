{
  "name": "harryzcy.github.io",
  "description": "This is where my website stays",
  "author": "Chongyi Zheng",
  "license": "UNLICENSED",
  "scripts": {
    "start": "parcel './src/*.html'",
    "build": "npm run build:parcel && npm run build:copy",
    "build:parcel": "parcel build",
    "build:copy": "cp src/projects.yaml dist && cp cloudflare/* dist",
    "analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "check": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue src",
    "prettier": "prettier src --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/index.html",
    "src/projects.html"
  ],
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "homepage": "https://github.com/harryzcy/harryzcy.github.io#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231218.0",
    "@parcel/packager-raw-url": "2.10.3",
    "@parcel/reporter-bundle-analyzer": "2.10.3",
    "@parcel/transformer-vue": "2.10.3",
    "@parcel/transformer-webmanifest": "2.10.3",
    "@parcel/transformer-yaml": "2.10.3",
    "@rushstack/eslint-patch": "1.6.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-vue": "9.19.2",
    "parcel": "2.10.3",
    "postcss": "8.4.32",
    "postcss-import": "15.1.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "typescript": "5.3.3",
    "vue-tsc": "1.8.27"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.16",
    "@heroicons/vue": "2.1.1",
    "tailwindcss": "3.4.0",
    "vue": "3.3.13"
  }
}
