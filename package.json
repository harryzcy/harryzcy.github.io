{
  "name": "harryzcy.github.io",
  "description": "This is where my website stays",
  "author": "Chongyi Zheng",
  "license": "UNLICENSED",
  "scripts": {
    "start": "parcel './src/*.html'",
    "build": "parcel build && cp cloudflare/* dist",
    "analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "check": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue src",
    "prettier": "prettier src --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/index.html",
    "src/projects.html"
  ],
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "homepage": "https://github.com/harryzcy/harryzcy.github.io#readme",
  "devDependencies": {
    "@parcel/packager-raw-url": "2.10.2",
    "@parcel/reporter-bundle-analyzer": "2.10.2",
    "@parcel/transformer-vue": "2.10.2",
    "@parcel/transformer-webmanifest": "2.10.2",
    "@parcel/transformer-yaml": "2.10.2",
    "@rushstack/eslint-patch": "1.5.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "9.18.1",
    "parcel": "2.10.2",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.7",
    "typescript": "5.2.2",
    "vue-tsc": "1.8.22"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.16",
    "@heroicons/vue": "2.0.18",
    "tailwindcss": "3.3.5",
    "vue": "3.3.8"
  }
}
