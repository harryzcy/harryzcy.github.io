{
  "name": "harryzcy.github.io",
  "description": "This is where my website stays",
  "author": "Chongyi Zheng",
  "license": "UNLICENSED",
  "scripts": {
    "start": "parcel './src/*.html'",
    "build": "parcel build",
    "analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "check": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue src",
    "prettier": "prettier --write src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/index.html",
    "src/projects.html"
  ],
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "homepage": "https://github.com/harryzcy/harryzcy.github.io#readme",
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "2.9.3",
    "@parcel/transformer-vue": "2.9.3",
    "@parcel/transformer-yaml": "2.9.3",
    "@rushstack/eslint-patch": "1.3.3",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "autoprefixer": "10.4.15",
    "eslint": "8.48.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "9.17.0",
    "parcel": "2.9.3",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "typescript": "5.2.2",
    "vue-tsc": "1.8.8"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.16",
    "@heroicons/vue": "2.0.18",
    "tailwindcss": "3.3.3",
    "vue": "3.3.4"
  }
}
